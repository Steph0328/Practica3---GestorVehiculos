@model GestorVehiculosDAL.Models.Vehiculo

@{
    ViewBag.Title = "Detalle de Vehículo";
}

<h2>@ViewBag.Title</h2>

<div>
    <h4>Vehículo @Model.Id</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Marca</dt>
        <dd>@Model.Marca</dd>

        <dt>Modelo</dt>
        <dd>@Model.Modelo</dd>

        <dt>Año</dt>
        <dd>@Model.Anio</dd>

        <dt>Precio</dt>
        <dd>@Model.Precio</dd>

        <dt>Color</dt>
        <dd>@Model.Color</dd>

        <dt>Placa</dt>
        <dd>@Model.Placa</dd>
    </dl>
</div>

<p>
    @Html.ActionLink("Volver al listado", "Index", "VehiculosWeb")
</p>
